# Easier navigation: .., ..., ...., ....., ~ and -
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

alias ls='ls -F --color'
alias ll='ls -l'
alias lla='ls -lA'
alias grep='grep --color'
alias diff='diff --color'
alias dotfiles-update='bash ~/dotfiles/update.sh'
